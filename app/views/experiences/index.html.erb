<h1>Listing experiences</h1>

<table>
  <tr>
    <th>Resume</th>
    <th>User</th>
    <th>Institute name</th>
    <th>City</th>
    <th>State</th>
    <th>Since year</th>
    <th>End year</th>
    <th>Title</th>
    <th>Summary</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @experiences.each do |experience| %>
  <tr>
    <td><%= experience.resume_id %></td>
    <td><%= experience.user_id %></td>
    <td><%= experience.institute_name %></td>
    <td><%= experience.city %></td>
    <td><%= experience.state %></td>
    <td><%= experience.since_year %></td>
    <td><%= experience.end_year %></td>
    <td><%= experience.title %></td>
    <td><%= experience.summary %></td>
    <td><%= link_to 'Show', experience %></td>
    <td><%= link_to 'Edit', edit_experience_path(experience) %></td>
    <td><%= link_to 'Destroy', experience, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Experience', new_experience_path %>
